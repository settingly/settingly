{
  "name": "frontend-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest --passWithNoTests",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness",
    "lint:eslint": "eslint . --fix --config eslint.config.ts",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "ci-all": "run-s lint:oxlint lint:eslint type-check test:unit"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@jaseeey/vue-umami-plugin": "^1.3.0",
    "@sentry/vue": "^9.15.0",
    "@tailwindcss/vite": "^4.1.4",
    "@unhead/vue": "^2.0.8",
    "lucide-vue-next": "^0.503.0",
    "pinia": "^3.0.1",
    "pocketbase": "^0.26.0",
    "unplugin-vue-markdown": "^28.3.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1",
    "vue-sonner": "^1.3.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vitest/eslint-plugin": "^1.1.39",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.1",
    "eslint-plugin-oxlint": "^0.16.8",
    "eslint-plugin-vue": "~10.1.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^8.0.3",
    "oxlint": "^0.16.8",
    "postcss": "^8.5.3",
    "prettier": "3.5.3",
    "tailwindcss": "^4.1.5",
    "typescript": "~5.8.0",
    "vite": "^6.3.4",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vitest": "^3.1.2",
    "vue-tsc": "^2.2.10"
  }
}
